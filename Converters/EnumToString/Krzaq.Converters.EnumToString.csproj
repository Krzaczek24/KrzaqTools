	</PropertyGroup>

	<ItemGroup>
		<None Include="README.md" Pack="true" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Krzaq.Attributes.EnumParse" Version="1.0.0" />
	  <PackageReference Include="Krzaq.Attributes.EnumToString" Version="1.0.0" />
	  <PackageReference Include="Krzaq.Extensions.StringNotation" Version="1.0.0" />
	</ItemGroup>

</Project>
